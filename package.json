{
  "name": "@pfantato/printful-ts",
  "version": "0.0.9",
  "description": "Typescript SDK to integrate with Printful",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "npx vitest --typecheck",
    "test:coverage": "npx vitest --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm t && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish": "npm publish --access=public",
    "update-mock": "npx msw-auto-mock ./files/printful-openapi.json -o ./tests/mocks/printful --typescript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfantato/printful-ts.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "printful",
    "typescript",
    "sdk",
    "printful-sdk",
    "typescript-sdk",
    "printful-ts",
    "printful-typescript",
    "printful-js",
    "javascript",
    "typescript",
    "javascript-sdk",
    "js-sdk",
    "ts-sdk"
  ],
  "author": "Patrick Fantato <me@pfantato.space>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pfantato/printful-ts/issues"
  },
  "homepage": "https://github.com/pfantato/printful-ts#readme",
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@hapi/bourne": "^3.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitest/coverage-istanbul": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "color-string": "^2.0.1",
    "dayjs": "^1.11.13",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vitest": "^0.5.4",
    "ky": "^1.7.5",
    "lodash": "^4.17.21",
    "msw": "^2.7.3",
    "msw-auto-mock": "^0.28.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "pino-tee": "^0.3.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8",
    "zod": "^3.24.2",
    "zod-schema-faker": "^1.5.2",
    "zod-validation-error": "^3.4.0"
  },
  "msw": {
    "workerDirectory": [
      "./public"
    ]
  }
}
