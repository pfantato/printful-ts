{
  "name": "@pfantato/printful-ts",
  "version": "0.0.23",
  "description": "Typescript SDK to integrate with Printful",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsup",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "npx vitest --typecheck",
    "test:coverage": "npx vitest --coverage",
    "prepublishOnly": "npm t && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish": "npm publish --access=public",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfantato/printful-ts.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "printful",
    "typescript",
    "sdk",
    "printful-sdk",
    "typescript-sdk",
    "printful-ts",
    "printful-typescript",
    "printful-js",
    "javascript",
    "typescript",
    "javascript-sdk",
    "js-sdk",
    "ts-sdk"
  ],
  "author": {
    "email": "me@pfantato.space",
    "name": "Patrick Fantato",
    "url": "https://github.com/pfantato"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pfantato/printful-ts/issues"
  },
  "homepage": "https://github.com/pfantato/printful-ts#readme",
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@hapi/bourne": "^3.0.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/coverage-v8": "^3.0.9",
    "color-string": "^2.0.1",
    "dayjs": "^1.11.13",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.5",
    "eslint-plugin-vitest": "^0.5.4",
    "ky": "^1.7.5",
    "lodash": "^4.17.21",
    "msw": "^2.7.3",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    }
  }
}
